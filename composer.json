{
  "name": "sujanshresthanet/esewa-php-sdk",
  "type": "library",
  "description": "eSewa payment gateway integration in PHP.",
  "homepage": "https://developer.esewa.com.np",
  "license": "MIT",
  "authors": [
    {
      "name": "Sujan Shrestha",
      "homepage": "https://sujanshrestha.net"
    }
  ],
  "keywords": [
    "api",
    "esewa",
    "gateway",
    "nepal",
    "payment",
    "php"
  ],
  "require": {
    "php": ">=7.2",
    "ext-SimpleXML": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "vlucas/phpdotenv": "^5.1"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {},
  "autoload": {
    "psr-4": {
      "Cixware\\Esewa\\": "src/"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {}
}
